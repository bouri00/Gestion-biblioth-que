
@model IEnumerable<Admin_pro.Models.Editeur>


<div class="right_col" role="main">
    <div class="col-md-12 col-sm-12 col-xs-12">
        <div class="x_panel">
            <div class="x_title">
                <h2>Liste Editeur <small>Users</small></h2>




                <ul class="nav navbar-right panel_toolbox">
                    <li>
                        <a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
                    </li>

                    <li class="dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false"><i class="fa fa-wrench"></i></a>
                        <ul class="dropdown-menu" role="menu">
                            <li>
                                <a href="#">Settings 1</a>
                            </li>
                            <li>
                                <a href="#">Settings 2</a>
                            </li>
                        </ul>
                    </li>

                    <li>
                        <a class="close-link"><i class="fa fa-close"></i></a>
                    </li>
                    <li>
                        <!-- Large modal -->
                        <button type="button" class="btn btn-primary BtnMymodaledit" data-toggle="modal" data-url="/Editeur/Create">Nouveau</button>
                    </li>

                </ul>
                <div class="clearfix"></div>
            </div>


            <table id="datatable-buttons" class="table table-striped table-bordered">


                <thead>
                    <tr>
                        <th>ID Iditeur : </th>
                        <th>Nom </th>
                        <th>Prenom </th>

                        <th> Email </th>
                        <th> Ville</th>
                        <th> Nationalite</th>
                        <th> Action</th>
                        

                    </tr>
                </thead>

                <tbody>

                    @foreach (var aa in Model)
                    {
                        <tr>
                            <td>@aa.id_Editeur</td>
                            <td>@aa.nomE</td>
                            <td> @aa.prenomE</td>
                            <td> @aa.Email</td>
                            <td> @aa.ville</td>
                            <td> @aa.nationalite</td>
                          
                            <td>

                                <button class="btn btn-primary btn-xs BtnMymodaledit" data-toggle="modal" data-url="/Editeur/Create?id_Editeur=@aa.id_Editeur"><i class="fa fa-folder"></i> Afficher </button>
                                <button href="#" class="btn btn-info btn-xs BtnMymodaledit" data-toggle="modal" data-url="/Editeur/Create?id_Editeur=@aa.id_Editeur"><i class="fa fa-pencil"></i> Modifer </button>
                                <button class="btn btn-danger btn-xs BtnMymodaledit" data-toggle="modal" data-url="/Editeur/Delete?id_Editeur=@aa.id_Editeur"><i class="fa fa-trash-o"></i> Supprimer </button>
                            </td>
                        </tr>

                    }
                </tbody>


            </table>



        </div>
    </div>
</div>


<span id="Modal_Cruded">
</span>

<script>
    $('.BtnMymodaledit').click(function () {
        var url = $(this).attr('data-url');
        $('#Modal_Cruded').html('');
        $.get(url, function (data) {

            $('#Modal_Cruded').html(data);
            $('#Mymodaledit').modal('show');
        });
    });
</script>





