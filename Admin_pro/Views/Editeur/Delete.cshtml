
@{
    Layout = null;
}

@model Admin_pro.ViewModels.EditeurViewModel



<div class="modal fade bs-example-modal-lg" tabindex="-1" role="dialog" aria-hidden="true" id="Mymodaledit">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">
                    <span aria-hidden="true">×</span>
                </button>
                <h4 class="modal-title" id="myModalLabel"> Supprimer Editeur :</h4>
            </div>
          <form id="demo-form2" data-parsley-validate class="form-horizontal form-label-left" action="/Editeur/Delete?id_Editeur=@Model.id_Editeur&conf=true" method="post">
                @Html.HiddenFor(x => x.id_Editeur)
            <div class="modal-body">
                Vous Voulez Vraiment Supprimer Cet Editeur ?
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                <button type="submit" class="btn btn-primary">Save changes</button>

            </div>
              </form>
        </div>
    </div>
</div>
